{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.1",
    "@builder.io/qwik-city": "^1.5.1",
    "@css-hooks/qwik": "^1.8.2",
    "@css-hooks/recommended": "^1.8.2",
    "@fontsource-variable/inconsolata": "^5.0.17",
    "@fontsource-variable/montserrat": "^5.0.18",
    "@fontsource-variable/onest": "^5.0.3",
    "@fontsource/montserrat": "^5.0.17",
    "@fontsource/onest": "^5.0.3",
    "@resvg/resvg-js": "^2.6.0",
    "@types/cheerio": "^0.22.35",
    "@types/eslint": "^8.56.5",
    "@types/jstoxml": "^2.0.4",
    "@types/node": "^20.11.25",
    "@types/slug": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.1",
    "gray-matter": "^4.0.3",
    "jstoxml": "^3.2.10",
    "marked": "^11.2.0",
    "prettier": "^3.2.5",
    "reading-time": "~1.5.0",
    "satori": "^0.10.13",
    "satori-html": "^0.3.2",
    "shikiji": "^0.9.19",
    "slug": "^8.2.3",
    "typescript": "^5.4.2",
    "undici": "^5.28.3",
    "varsace": "^0.2.1",
    "vite": "^5.1.5",
    "vite-plugin-arraybuffer": "^0.0.6",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
